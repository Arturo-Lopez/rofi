{
  "name": "@rofi/api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only -r dotenv/config src/index | pino-pretty",
    "dev:debug": "ts-node-dev --inspect=4321 --respawn --transpile-only src/index",
    "start": "node dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schemas/rootSchema",
    "db:push": "prisma db push",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "test": "jest",
    "lint": "eslint src/ --ext .ts"
  },
  "dependencies": {
    "@fastify/session": "^6.3.0",
    "@prisma/client": "^3.7.0",
    "@rofi/config": "*",
    "altair-fastify-plugin": "^4.3.1",
    "argon2": "^0.28.3",
    "aws-sdk": "^2.1052.0",
    "axios": "^0.24.0",
    "fastify": "^3.25.3",
    "fastify-cookie": "^5.5.0",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.0",
    "fastify-redis": "^4.3.3",
    "google-auth-library": "^7.11.0",
    "graphql": "^16.2.0",
    "graphql-middleware": "^6.1.13",
    "graphql-scalars": "^1.14.1",
    "graphql-shield": "^7.5.0",
    "graphql-upload": "^13.0.0",
    "mercurius": "^9.1.0",
    "mercurius-cache": "^0.11.0",
    "mercurius-upload": "^3.0.1",
    "nexus": "^1.2.0-next.17",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chance": "^1.1.8",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript-base": "^3.0.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.7",
    "pino-pretty": "^7.3.0",
    "prisma": "^3.7.0",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
